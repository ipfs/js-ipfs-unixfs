{
  "name": "ipfs-unixfs-exporter",
  "version": "5.0.3",
  "description": "JavaScript implementation of the UnixFs exporter used by IPFS",
  "leadMaintainer": "Alex Potsides <alex.potsides@protocol.ai>",
  "main": "src/index.js",
  "browser": {
    "fs": false
  },
  "scripts": {
    "prepare": "aegir build --no-bundle",
    "test": "aegir test",
    "build": "aegir build",
    "clean": "rimraf ./dist",
    "lint": "aegir ts --check && aegir lint",
    "coverage": "nyc -s npm run test -t node && nyc report --reporter=html",
    "depcheck": "aegir dep-check -i @types/mocha -i @types/sinon -i nyc -i abort-controller -i rimraf -i ipfs-core-types -i copy -i util -i crypto-browserify -i events -i readable-stream"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-ipfs-unixfs.git"
  },
  "keywords": [
    "IPFS"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs/js-ipfs-unixfs/issues"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "homepage": "https://github.com/ipfs/js-ipfs-unixfs#readme",
  "devDependencies": {
    "@types/mocha": "^8.2.1",
    "@types/sinon": "^10.0.0",
    "abort-controller": "^3.0.0",
    "aegir": "^33.1.0",
    "copy": "^0.3.2",
    "crypto-browserify": "^3.12.0",
    "detect-node": "^2.0.4",
    "events": "^3.3.0",
    "ipfs-core-types": "^0.3.1",
    "ipfs-unixfs-importer": "^7.0.3",
    "ipld": "^0.29.0",
    "ipld-block": "^0.11.1",
    "ipld-dag-pb": "^0.22.2",
    "ipld-in-memory": "^8.0.0",
    "it-all": "^1.0.5",
    "it-buffer-stream": "^2.0.0",
    "it-first": "^1.0.6",
    "merge-options": "^3.0.4",
    "multicodec": "^3.0.1",
    "native-abort-controller": "^1.0.3",
    "nyc": "^15.0.0",
    "readable-stream": "^3.6.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.0",
    "uint8arrays": "^2.1.2",
    "util": "^0.12.3"
  },
  "dependencies": {
    "cids": "^1.1.5",
    "err-code": "^3.0.1",
    "hamt-sharding": "^2.0.0",
    "ipfs-unixfs": "^4.0.3",
    "it-last": "^1.0.5",
    "multihashing-async": "^2.1.0"
  },
  "types": "dist/src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "eslintConfig": {
    "extends": "ipfs"
  }
}
